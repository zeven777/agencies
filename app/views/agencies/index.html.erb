          <h2 class="blueColor">${{}}</h2>

          <h5 class="redColor">Q1 YTD GOAL</h5>
          <h3 class="redColor">${{}}</h3>
          </div>
        <div id="chartpies"></div>
        <div class="pietext">
          <h2>${{}}</h2>
          <h4>Q1 Goal</h4>
        </div>
      </div>
      <div class="col-md-6">
        <div class="title"><h4>Sales Comparison</h4></div>
        <div id="chartdiv"></div>
        <div id="chartyears"></div>
      </div>
    </div>
  </div>
  <!-- +++++++++++++++++++++++++++++ -->



