<div class="container" ng-app="myApp" ng-controller="AgencyController">
  <div class="row">
    <h2>Pie values:</h2>
    <div class="form-inline">
      <div class="form-group">
        <label>Q1 YTD SALES %</label>
        <input type="text" class="form-control" ng-model="pie1" />
      </div>
      <div class="form-group">
        <label>Q1 YTD GOAL %</label>
        <input type="text" class="form-control" ng-model="pie2" />
      </div>
    </div>
    <br/>
    <div class="form-inline">
      <div class="form-group">
        <label>Q1 YTD SALES</label>
        <input type="text" class="form-control" ng-model="pie3" />
      </div>
      <div class="form-group">
        <label>Q1 YTD GOAL</label>
        <input type="text" class="form-control" ng-model="pie4" />
      </div>
      <label>Q1 GOAL</label>
      <div class="form-group">
        <input type="text" class="form-control" ng-model="pie5" />
      </div>
    </div>
    <br/>

  </div>
  <br/>

  <h3>Select Agency</h3>

  <div class="container-fluid">
    <form class="form-inline">
      <button ng-repeat="x in linkAgency track by $index" type="button" class="btn btn-primary" value="{{x}}" ng-click="pedirValor(x)">{{x}}</button>
    </form>
  </div>


  <!-- +++++++++++++++ Start display chart +++++++++++++++ -->

  <h1 class="page-header">Display charts</h1>



  <div id="chartsDiv" class="saveChart">

    <div class="row">
      <div class="col-md-6">
        <div class="tagPie">
          <h4 class="blueColor">Q1 YTD SALES</h4>
          <h2 class="blueColor">${{pie3}}</h2>

          <h5 class="redColor">Q1 YTD GOAL</h5>
          <h3 class="redColor">${{pie4}}</h3>
          </div>
        <div id="chartpies"></div>
        <div class="pietext">
          <h2>${{pie5}}</h2>
          <h4>Q1 Goal</h4>
        </div>
      </div>
      <div class="col-md-6">
        <div class="title"><h4>Sales Comparison</h4></div>
        <div class="col-md-2"></div>
        <div class="col-md-5 texTitleBl"><p>Economy</p></div>
        <div class="col-md-5 texTitleBd"><p>Premium</p></div>
        <div id="chartdiv"></div>
        <div id="chartyears"></div>
      </div>
    </div>
  </div>
  <!-- +++++++++++++++++++++++++++++ -->


  <table class="table table-bordered">
    <thead>
      <tr>
        <th class="tTop">Months</th>
        <th class="tTop">2014</th>
        <th class="tTop">2015</th>
        <th class="tTop"><>(US$)</th>
        <th class="tTop">r'LY</th>
        <th class="tTop">Goal</th>
        <th class="tTop">r'Goal</th>
        <th class="tTop">LH</th>
        <th class="tTop">r'LY</th>
        <th class="tTop">REG</th>
        <th class="tTop">r'LY</th>
        <th class="tTop">DOM</th>
        <th class="tTop">r'LY</th>
        <th class="tTop">Other</th>
        <th class="tTop">r'LY</th>
      </tr>
    </thead>
    <tbody>
      <tr>
        <th scope="row">Jan</th>
        <th><p class="content" contenteditable="true">{{lyViewJan}}</p></th>
        <th><p class="content" contenteditable="true">{{ayViewJan}}</p></th>
        <th class="colGr"><p class="content" contenteditable="true">{{usDollarJan}}</p></th>
        <th><p class="content" contenteditable="true">{{rLYJan}}%</p></th>
        <th class="colGr"><p class="content" contenteditable="true">{{goalTabJan}}</p></th>
        <th><p class="content" contenteditable="true">{{rgoalTabJan}}%</p></th>
        <th><p class="content" contenteditable="true">{{viewLHJan}}</p></th>
        <th class="colGr"><p class="content" contenteditable="true">{{rLYLHJan}}%</p></th>
        <th><p class="content" contenteditable="true">{{regJan}}</p></th>
        <th class="colGr"><p class="content" contenteditable="true">{{rREGJan}}%</p></th>
        <th><p class="content" contenteditable="true">{{domJan}}</p></th>
        <th class="colGr"><p class="content" contenteditable="true">{{rDOMJan}}%</p></th>
        <th><p class="content" contenteditable="true">{{otherJan}}</p></th>
        <th class="colGr"><p class="content" contenteditable="true">{{rOTHERJan}}%</p></th>
      </tr>

      <tr>
        <th scope="row">Feb</th>
        <th><p class="content" contenteditable="true">{{lyViewFeb}}</p></th>
        <th><p class="content" contenteditable="true">{{ayViewFeb}}</p></th>
        <th class="colGr"><p class="content" contenteditable="true">{{usDollarFeb}}</p></th>
        <th><p class="content" contenteditable="true">{{rLYFeb}}%</p></th>
        <th class="colGr"><p class="content" contenteditable="true">{{goalTabFeb}}</p></th>
        <th><p class="content" contenteditable="true">{{rgoalTabFeb}}%</p></th>
        <th><p class="content" contenteditable="true">{{viewLHFeb}}</p></th>
        <th class="colGr"><p class="content" contenteditable="true">{{rLYLHFeb}}%</p></th>
        <th><p class="content" contenteditable="true">{{regFeb}}</p></th>
        <th class="colGr"><p class="content" contenteditable="true">{{rREGFeb}}%</p></th>
        <th><p class="content" contenteditable="true">{{domFeb}}</p></th>
        <th class="colGr"><p class="content" contenteditable="true">{{rDOMFeb}}%</p></th>
        <th><p class="content" contenteditable="true">{{otherFeb}}</p></th>
        <th class="colGr"><p class="content" contenteditable="true">{{rOTHERFeb}}%</p></th>
      </tr>

      <tr>
        <th scope="row">Mar</th>
        <th><p class="content" contenteditable="true">{{lyViewMar}}</p></th>
        <th><p class="content" contenteditable="true">{{ayViewMar}}</p></th>
        <th class="colGr"><p class="content" contenteditable="true">{{usDollarMar}}</p></th>
        <th><p class="content" contenteditable="true">{{rLYMar}}%</p></th>
        <th class="colGr"><p class="content" contenteditable="true">{{goalTabMar}}</p></th>
        <th><p class="content" contenteditable="true">{{rgoalTabMar}}%</p></th>
        <th><p class="content" contenteditable="true">{{viewLHMar}}</p></th>
        <th class="colGr"><p class="content" contenteditable="true">{{rLYLHMar}}%</p></th>
        <th><p class="content" contenteditable="true">{{regMar}}</p></th>
        <th class="colGr"><p class="content" contenteditable="true">{{rREGFeb}}%</p></th>
        <th><p class="content" contenteditable="true">{{domMar}}</p></th>
        <th class="colGr"><p class="content" contenteditable="true">{{rDOMMar}}%</p></th>
        <th><p class="content" contenteditable="true">{{otherMar}}</p></th>
        <th class="colGr"><p class="content" contenteditable="true">{{rOTHERMar}}%</p></th>
      </tr>

    </tbody>
  </table>
